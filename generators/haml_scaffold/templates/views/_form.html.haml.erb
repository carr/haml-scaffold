<% attributes.reject!{|x| ['created_by', 'updated_by', 'position', "#{ActiveRecord::ScopeEverything.field}"].member?(x.name)} -%>
- semantic_form_for(<%= singular_name %>) do |f|
  - f.inputs do
<% for attribute in attributes -%>
    = f.input :<%= attribute.name %>
<% end -%>
  - f.buttons do
    = f.commit_button

